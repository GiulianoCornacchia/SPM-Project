-L/usr/X11R6/lib -lm -lpthread -lX11 -std=gnu++11

compile
  seq
	g++ test.cpp -D cimg_display=0 -lm -lpthread -std=gnu++11 -o test	
  par
	c++ farm_pipes.cpp -D cimg_display=0 -lm -lpthread -std=gnu++11 -o farm
exe
  seq
	./test ./imgs/source/ ./imgs/result/ ./imgs/watermark.jpg n_imgs
  par
	./farm ./imgs/source/ ./imgs/result/ ./imgs/watermark.jpg par_deg n_imgs


